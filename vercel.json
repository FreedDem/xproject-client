{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://<ТВОЙ-ДОМЕН-СЕРВЕРА-AMVERA>/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://<ТВОЙ-ДОМЕН-СЕРВЕРА-AMVERA>/uploads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
